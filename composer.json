{
    "name": "als/exception",
    "type": "library",
    "description": "Parser3 Exceptions Renderer.",
    "keywords": ["parser3", "exception"],
    "homepage": "https://github.com/parser3/als.exception",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonid Knyazev",
            "email": "leonid@knyazev.me",
            "homepage": "https://knyazev.me",
            "role": "Developer"
        }
    ],
    "support": {
